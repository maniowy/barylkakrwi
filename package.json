{
  "name": "barylka",
  "version": "0.0.1",
  "description": "Barylka krwi",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url client/styles.scss public/css/styles.css",
    "css-postcss": "postcss --map false --output public/css/styles.css public/css/styles.css",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-watch": "npm run css-build -- --watch",
    "js-build": "babel _javascript --out-dir lib",
    "js-watch": "npm run js-build -- --watch",
    "deploy": "npm run css-deploy && npm run js-build",
    "test": "mocha",
    "start": "DEBUG=*:* npx nodemon server.js"
  },
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "axios": "^0.24.0",
    "bulma": "^0.9.3",
    "cookie-parser": "^1.4.6",
    "css-byebye": "^3.1.0",
    "express": "^4.17.2",
    "filereader": "^0.10.3",
    "form-data": "^4.0.0",
    "formdata-node": "^2.5.0",
    "formidable": "^1.2.2",
    "md5": "^2.3.0",
    "mocha": "^9.2.0",
    "node-sass": "^7.0.1",
    "postcss-discard-empty": "^5.0.3",
    "qs": "^6.10.3",
    "simple-node-logger": "^18.12.24"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "postcss": "^8.4.6",
    "postcss-cli": "^8.3.1",
    "pug": "^3.0.2"
  }
}
