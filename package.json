{
  "name": "barylka",
  "version": "0.0.1",
  "description": "Barylka krwi",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url client/styles.scss public/css/styles.css",
    "css-postcss": "postcss --map false --output public/css/styles.css public/css/styles.css",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-watch": "npm run css-build -- --watch",
    "js-build": "babel public/js --out-dir lib",
    "js-watch": "npm run js-build -- --watch",
    "deploy": "npm run css-deploy && npm run js-build",
    "test": "mocha",
    "start": "DEBUG=*:* npx nodemon --inspect server.js",
    "start_windows": "set DEBUG=*:* & npx nodemon --inspect server.js"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.4",
    "bulma": "^0.9.4",
    "cookie-parser": "^1.4.6",
    "css-byebye": "^4.0.1",
    "express": "^4.17.2",
    "express-requests-logger": "^3.0.4",
    "filereader": "^0.10.3",
    "formidable": "^2.1.1",
    "md5": "^2.3.0",
    "mocha": "^10.2.0",
    "node-sass": "^8.0.0",
    "postcss-discard-empty": "^5.0.3",
    "qs": "^6.10.3",
    "simple-node-logger": "^21.8.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.6",
    "postcss-cli": "^10.1.0",
    "pug": "^3.0.2"
  }
}
